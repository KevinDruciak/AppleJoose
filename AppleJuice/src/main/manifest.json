{
  "name": "Apple Juice",
  "version": "0.1",
  "description": "Apple-jOOSE Extension!",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./javaScript/content.js"]
    }
  ],
  "browser_action": {

    "default_title": "AppleJOOSE"

  },
  "background": {
    "scripts": ["./javaScript/background.js"]
  },
  "manifest_version": 2,
  "permissions": ["tabs", "cookies"]
}