{
  "name": "Know Your Bias",
  "version": "1.0",
  "description": "Apple-jOOSE Extension!",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["javaScript/model/background.js"],
    "persistent": true
  },

  "browser_action": {
    "default_popup": "html/view/popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "manifest_version": 2
}
